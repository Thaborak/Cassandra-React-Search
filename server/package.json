{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express backend w/cassandra",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "mocha": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js'",
    "mocha-exit0": "mocha --exit --file test/setup.test.js '!(node_modules)/**/*test.js' || true",
    "test": "nodemon --exec npm run mocha-exit0",
    "cover": "nodemon --exec nyc --reporter=lcov --reporter=text-summary npm run mocha-exit0",
    "heroku": "node index.js",
    "travis": "npm run mocha"
  },
  "dependencies": {
    "cassandra-driver": "*",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-cassandra": "^2.2.2",
    "gdax": "^0.8.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  }
}
